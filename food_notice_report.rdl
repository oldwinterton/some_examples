        <CommandText>---- Food Notice Report (https://redmine.scubeenterprise.com/issues/19509)
-----------------------------------------
---- Description: Food Notice Report - address_info
---- Version:1.0
-----------------------------------------
---- Created: 2024-07-12 - js - Notes
---- Modified: 2024-07-12 - js - Notes
-- 
-----------------------------------------
---- Development Code
---- DECLARE Statements below this
--DECLARE @inspection_id int;
-- 
---- SET Statements below this
--set @inspection_id = 13098;
-- 
-----------------------------------------
----Main SQL below this


select
	b.B1_CHECKLIST_COMMENT as [name of establishment?] --5
	,coalesce(o.B1_FULL_NAME,'Establishment Owner') as owner_name		   --9,10
	,addr.B1_HSE_NBR_START as bldg_number --9,12
	,addr.B1_STR_DIR as street_direction	   --9,12
	,addr.b1_str_name as street_name	   --9,12
	,addr.b1_unit_type as unit_type 	   --9,12
	,addr.B1_UNIT_START as unit_number	   --9,12
	,addr.B1_SITUS_CITY as city	   --9,12
	,addr.B1_SITUS_STATE as state  --9,12
	,addr.B1_SITUS_ZIP as zip	   --9,12
	,trim(
		coalesce(     convert(varchar,addr.B1_HSE_NBR_START),'')
		+coalesce(' '+convert(varchar,addr.B1_STR_DIR      ),'')
		+coalesce(' '+convert(varchar,addr.b1_str_name     ),'')
		+coalesce(' '+convert(varchar,addr.b1_unit_type    ),'')
		+coalesce(' '+convert(varchar,addr.B1_UNIT_START   ),'')
	) as address_line1
from G6ACTION a --on concat(i.b1_per_id1,i.b1_per_id2,i.b1_per_id3) = concat(a.b1_per_id1,a.b1_per_id2,a.b1_per_id3)
left join BCHCKBOX b on b.serv_prov_code = a.serv_prov_code and concat(b.b1_per_id1,b.b1_per_id2,b.b1_per_id3) = concat(a.b1_per_id1,a.b1_per_id2,a.b1_per_id3) and b.B1_CHECKBOX_DESC_ALIAS = 'Name of Establishment?'
left join B3CONTACT o on o.serv_prov_code = a.serv_prov_code and concat(o.b1_per_id1,o.b1_per_id2,o.b1_per_id3) = concat(a.b1_per_id1,a.b1_per_id2,a.b1_per_id3) and o.B1_CONTACT_TYPE = 'Establishment Owner'
left join B3ADDRES addr on addr.serv_prov_code = a.serv_prov_code and concat(addr.b1_per_id1,addr.b1_per_id2,addr.b1_per_id3) = concat(a.b1_per_id1,a.b1_per_id2,a.b1_per_id3)
where
	1=1
	and a.serv_prov_code = 'DAVISCO'
	and a.g6_act_num = @inspection_id
;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
