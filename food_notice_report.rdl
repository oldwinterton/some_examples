        <CommandText>---- ReportName (https://redmine.scubeenterprise.com/issues/19509)
-----------------------------------------
---- Description: Food Notice Report - inspector_list
---- Version:1.0
-----------------------------------------
---- Created:2024-07-17 - js - inspector_list -- This query is used to populate the parameter dropdown "user_name". Therefore it must use &amp;user_id to match the existing user. Is this problematic?
---- Modified: 2024-07-17 - js - Notes
--
---------------------------------------
---- Development Code
---- DECLARE Statements below this
--declare @user_id varchar(100); 
--
---- SET Statements below this
--set @user_id = 'joelstewart'
-- 
-----------------------------------------
----Main SQL below this

select
	inspector_name
	,inspector_title
	,inspector_email
from (
	select 
		trim(replace(isnull(ga_fname,'')+' '+isnull(ga_lname,''),'  ',' ')) as inspector_name
		,GA_TITLE inspector_title
		,GA_EMAIL inspector_email
	from G3STAFFS
	where
		1=1
		and SERV_PROV_CODE = 'DAVISCO'
		and GA_BUREAU_CODE in ('INSPECT','ADMIN')
		and REC_STATUS = 'A'
	union
	select 
		@user_id
		,'' as title
		,'' as email
) a
order by case when inspector_name = @user_id then 1 else 2 end,1
;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="inspector_name">
          <DataField>inspector_name</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="inspector_title">
          <DataField>inspector_title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="inspector_email">
          <DataField>inspector_email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
