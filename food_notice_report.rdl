        <CommandText>use accelaconv1 
-- Food Notice Report (https://redmine.scubeenterprise.com/issues/19509)
---------------------------------------
-- Description: Food Notice Report (https://redmine.scubeenterprise.com/issues/19509)
-- Version:1.0
---------------------------------------
-- Created: 2024-07-12 - js - Notes
-- Modified: 2024-07-12 - js - Notes
 
---------------------------------------
------ Development Code
------ DECLARE Statements below this
--DECLARE @inspection_id int;
--
------ SET Statements below this
--set @inspection_id = 13098;
---------------------------------------
--Main SQL below this

;with isCos_isRpt as (
	select
		gi.GUIDE_ITEM_SEQ_NBR
		,max(case when asitv.asitab_name = 'COS' and asitv.ATTRIBUTE_VALUE = 'CHECKED' then 1 else 0 end) as isCos
		,max(case when asitv.asitab_name = 'RPT' and asitv.ATTRIBUTE_VALUE = 'CHECKED' then 1 else 0 end) as isRpt
	from G6ACTION a
	join b1permit b on b.SERV_PROV_CODE = a.SERV_PROV_CODE and concat(b.b1_per_id1,b.b1_per_id2,b.b1_per_id3) = concat(a.b1_per_id1,a.b1_per_id2,a.b1_per_id3)
	join GGUIDESHEET g on g.SERV_PROV_CODE = a.SERV_PROV_CODE and concat(g.b1_per_id1,g.b1_per_id2,g.b1_per_id3) = concat(a.b1_per_id1,a.b1_per_id2,a.b1_per_id3) and g.G6_ACT_NUM = a.g6_act_num
	join gguidesheet_item gi on gi.SERV_PROV_CODE = a.SERV_PROV_CODE and gi.GUIDESHEET_SEQ_NBR = g.GUIDESHEET_SEQ_NBR
	join GGDSHEET_ITEM_ASITAB_VALUE asitv on asitv.SERV_PROV_CODE = a.SERV_PROV_CODE and asitv.GUIDESHEET_SEQ_NBR = g.GUIDESHEET_SEQ_NBR and asitv.GUIDEITEM_SEQ_NBR = gi.GUIDE_ITEM_SEQ_NBR
	where
		1=1
		and a.SERV_PROV_CODE = 'DAVISCO'
		and a.g6_act_num = @inspection_id
		and ASITAB_SUBGRP_NAM = 'FOOD CHECKLIST'
		and ASITAB_NAME in ('COS','RPT')
	group by gi.GUIDE_ITEM_SEQ_NBR
)
select 
	i.G6_ACT_TYP as inspection_type
	,case 
		when try_convert(int,left(gi.guide_item_text,2)) is not null 
		then case 
			--This is based on the spec sheet. I compared the configured checklist for converted items to the checklist in nonprod1. 
			-- Converted counts headers; nonprod1 does not appear to count headers in backend as checklist items.
			--Therefore these conditions are based on the checklist items appearing with leading digits, ie '01) blah'.
			when convert(int,left(gi.guide_item_text,2)) between 1 and 56 then 'Utah Administrative Code, Rule R392-100. Food Service Sanitation.'
			when convert(int,left(gi.guide_item_text,2)) between 57 and 57 then 'Utah Administrative Code, Rule R392-103. Food Handler Training and Certificate.'
			when convert(int,left(gi.guide_item_text,2)) between 58 and 60 then 'Other Applicable Rules'
			else null
			end
		else case 
			--This is based on the sandbox. I compared the configured checklist for converted items to the checklist in nonprod1. 
			-- Converted counts headers; nonprod1 does not appear to count headers in backend as checklist items.
			when gi.GUIDE_ITEM_DISPLAY_ORDER between 1 and 74 then 'Utah Administrative Code, Rule R392-100. Food Service Sanitation.'
			when gi.GUIDE_ITEM_DISPLAY_ORDER between 76 and 76 then 'Utah Administrative Code, Rule R392-103. Food Handler Training and Certificate.'
			when gi.GUIDE_ITEM_DISPLAY_ORDER &gt;= 77 then 'Other Applicable Rules'
			else null
			end
		end
		as cat
	,gi.GUIDE_ITEM_DISPLAY_ORDER
	,gi.GUIDE_ITEM_COMMENT
	,'('+convert(varchar(100),row_number() OVER (PARTITION BY 
		case 
		when try_convert(int,left(gi.guide_item_text,2)) is not null 
		then case 
			when convert(int,left(gi.guide_item_text,2)) between 1 and 56 then 'Utah Administrative Code, Rule R392-100. Food Service Sanitation.'
			when convert(int,left(gi.guide_item_text,2)) between 57 and 57 then 'Utah Administrative Code, Rule R392-103. Food Handler Training and Certificate.'
			when convert(int,left(gi.guide_item_text,2)) between 58 and 60 then 'Other Applicable Rules'
			else null
			end
		else case 
			when gi.GUIDE_ITEM_DISPLAY_ORDER between 1 and 74 then 'Utah Administrative Code, Rule R392-100. Food Service Sanitation.'
			when gi.GUIDE_ITEM_DISPLAY_ORDER between 76 and 76 then 'Utah Administrative Code, Rule R392-103. Food Handler Training and Certificate.'
			when gi.GUIDE_ITEM_DISPLAY_ORDER &gt;= 77 then 'Other Applicable Rules'
			else null
			end
		end
		ORDER BY gi.guide_item_seq_nbr asc))+')' AS Rank
	,coalesce(isCos_isRpt.isCos,0) as isCos
	,coalesce(isCos_isRpt.isRpt,0) as isRpt
from GGUIDESHEET a
join GGUIDESHEET_ITEM gi on gi.SERV_PROV_CODE = a.SERV_PROV_CODE and gi.GUIDESHEET_SEQ_NBR = a.GUIDESHEET_SEQ_NBR
join G6ACTION i on i.SERV_PROV_CODE = a.SERV_PROV_CODE and i.g6_act_num = a.G6_ACT_NUM
left join isCos_isRpt on isCos_isRpt.GUIDE_ITEM_SEQ_NBR = gi.GUIDE_ITEM_SEQ_NBR	
where
	1=1
	and a.serv_prov_code = 'DAVISCO'
	and a.G6_ACT_NUM = @inspection_id
	and gi.guide_type = 'Food Establishment Inspection Checklist'
	and gi.GUIDE_ITEM_COMMENT is not null
order by gi.GUIDE_ITEM_DISPLAY_ORDER
;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="inspection_type">
          <DataField>inspection_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cat">
          <DataField>cat</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GUIDE_ITEM_DISPLAY_ORDER">
          <DataField>GUIDE_ITEM_DISPLAY_ORDER</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="GUIDE_ITEM_COMMENT">
          <DataField>GUIDE_ITEM_COMMENT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rank">
          <DataField>Rank</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="isCos">
          <DataField>isCos</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="isRpt">
          <DataField>isRpt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
